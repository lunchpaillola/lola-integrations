{
    "name": "frigg-app-back-end",
    "prettier": "@friggframework/prettier-config",
    "private": true,
    "scripts": {
        "test": "jest --group=-interactive --group=-live-api",
        "test:full": "jest",
        "test:process": "jest --testPathPattern process-tests process.test",
        "debug": "jest --testTimeout 0",
        "ci": "npm install",
        "lint": "eslint .",
        "lint:fix": "prettier --write --loglevel error . && eslint . --fix",
        "lint:cloudformation": "sls package --stage dev && cfn-lint .serverless/cloudformation-template-*.json",
        "lint:markdown": "markdownlint --fix .",
        "backend-start": "sls offline --stage dev",
        "backend-start:staging": "sls offline --stage staging",
        "docker:start": "docker-compose up -d",
        "docker:stop": "docker-compose down"
    },
    "dependencies": {
        "@friggframework/api-module-linear": "^1.0.0",
        "@friggframework/api-module-hubspot": "^1.0.0",
        "@friggframework/devtools": "^1.0.0",
        "@friggframework/core": "^1.0.0",
        "@hapi/boom": "^7.4.11",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "chai": "^4.3.7",
        "chai-http": "^4.3.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-async-handler": "^1.1.4",
        "lodash": "^4.17.15",
        "md5": "^2.3.0",
        "moment": "^2.27.0",
        "mongoose": "^6.2.1",
        "node-fetch": "^2.6.0",
        "serverless-http": "^2.5.0",
        "source-map-support": "^0.5.20"
    },
    "devDependencies": {
        "@friggframework/eslint-config": "^1.0.7",
        "@friggframework/prettier-config": "^1.0.5",
        "@friggframework/test-environment": "^1.1.4",
        "aws-sdk": "^2.1390.0",
        "copy-webpack-plugin": "^10.2.4",
        "faker": "^5.5.3",
        "jest": "^27.4.7",
        "markdownlint-cli": "^0.28.1",
        "dotenv": "^16.0.3",
        "nodemon": "^2.0.12",
        "open": "^7.1.0",
        "prettier": "^2.3.2",
        "proxyquire": "^2.1.3",
        "puppeteer": "^10.1.0",
        "server-destroy": "^1.0.1",
        "serverless": "^3.17.0",
        "serverless-offline": "^12.0.4",
        "serverless-webpack": "^5.7.1",
        "sinon": "^10.0.0",
        "sinon-chai": "^3.6.0",
        "supertest": "^6.2.4",
        "webpack": "^5.76.0"
    }
}
